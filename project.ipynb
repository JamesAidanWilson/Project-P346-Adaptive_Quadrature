    "def Int_Trapezoid(f,a,b,N):\n",
    "    h = float((b-a)/N)\n",
    "    i = 0\n",
    "    I = 0\n",
    "    while i != N:\n",
    "        I += (h/2)*(f(a + i*h) + f(a + (i + 1)*h))\n",
    "    return I"
   ]
  }
